<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.yatri">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Authentication.ProfileUserInformation"></activity>
        <activity android:name=".OutsideValley.ProfileOutsideValley" />
        <activity android:name=".VehicleTracking.ProfileVehicleTracking" />
        <activity android:name=".InsideValley.ProfileInsideValley" />
        <activity android:name=".Authentication.ProfileActivity" />
        <activity android:name=".Authentication.LoginActivity" />
        <activity android:name=".Authentication.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>